            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/python"
            },
            "args": [
                "{env:DATA_ROOT}/rpt-rivers-need-space",
                "/mnt/c/nardata/work/reporting/sample_gis/sample_geojsons/althouse_small_selection_1.geojson",
                "Althouse Creek 1"
            ]
        },
    ]
}