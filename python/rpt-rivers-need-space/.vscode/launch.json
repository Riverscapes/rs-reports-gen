{
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/python"
            },
        },
        {
            "name": "Python Debugger: rpt-rivers-need-space",
            "type": "debugpy",
